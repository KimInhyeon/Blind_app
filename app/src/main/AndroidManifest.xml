<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ksinfo.tomodomo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.Blind">
        <activity android:name="com.ksinfo.tomodomo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.ksinfo.tomodomo.member.MemberJoinActivity"/>
        <activity android:name="com.ksinfo.tomodomo.member.LoginActivity"/>
        <activity android:name="com.ksinfo.tomodomo.home.Home"/>
        <activity android:name="com.ksinfo.tomodomo.annualincome.AnnualIncomeRankCalculatorActivity"/>
        <activity android:name="com.ksinfo.tomodomo.mypage.Mypage"/>
        <activity android:name="com.ksinfo.tomodomo.board.TopicMainActivity"/>
        <activity android:name="com.ksinfo.tomodomo.controller.company.CompanyReviewActivity"/>
        <activity android:name="com.ksinfo.tomodomo.controller.company.CompanySearchActivity"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
</manifest>