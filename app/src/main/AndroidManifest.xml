<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ksinfo.tomodomo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Blind">
        <activity
            android:name=".controller.mypage.MyTaskActivity"
            android:exported="false" />
        <activity
            android:name=".controller.mypage.ModifyProfileActivity"
            android:exported="false" />
        <activity
            android:name=".controller.mypage.ModifyPasswordActivity"
            android:exported="false" />
        <activity
            android:name="com.ksinfo.tomodomo.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".controller.member.MemberJoinActivity" />
        <activity android:name=".controller.member.LoginActivity" />
        <activity android:name=".controller.home.Home" />
        <activity android:name=".controller.annualincome.AnnualIncomeRankCalculatorActivity" />
        <activity android:name=".controller.mypage.Mypage" />
        <activity android:name=".controller.board.TopicMainActivity" />
        <activity android:name=".controller.annualincome.AnnualIncomeRcViewSampleActivity"/>
    </application>

</manifest>